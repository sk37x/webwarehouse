                        .form-group.col-12
                            label ยืนยันรหัสผ่าน &nbsp; 
                                font(color='red')= (data !== null ? '': '*' )
                            input.form-control(value='', type='password', name='passwordConf', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                            span.error-block.invalid-feedback(data-from='passwordConf') กรุณากรอกข้อมูล
                    //- 
                        .form-group.col-12
                            label รหัสผ่านเดิม
                                font(color='red') &nbsp;*
                            input.form-control(value='', type='password', name='oldPassword', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                            span.error-block.invalid-feedback(data-from='oldPassword') กรุณากรอกข้อมูล
                if eventText === 'add'
                    .form-group.col-12
                        label รหัสผ่าน &nbsp; 
                            font(color='red')= (data !== null ? '': '*' )
                        input.form-control(value='', type='password', name='password', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                        span.error-block.invalid-feedback(data-from='password') กรุณากรอกข้อมูล
                    .form-group.col-12
                        label ยืนยันรหัสผ่าน &nbsp; 
                            font(color='red')= (data !== null ? '': '*' )
                        input.form-control(value='', type='password', name='passwordConf', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                        span.error-block.invalid-feedback(data-from='passwordConf') กรุณากรอกข้อมูล
                .form-row.mb-15(style='margin-left:10px !important;margin-right: 10px !important;')
                    .col-6
                        label ชื่่อ
                            font(color='red') &nbsp;*
                        input.form-control(value=(data !== null ? data.firstname:''), type='text', name='firstname', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                        span.error-block.invalid-feedback(data-from='firstname') กรุณากรอกข้อมูล
                    .col-6
                        label นามสกุล
                            font(color='red') &nbsp;*
                        input.form-control(value=(data !== null ? data.lastname:''), type='text', name='lastname', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                        span.error-block.invalid-feedback(data-from='lastname') กรุณากรอกข้อมูล
                .form-row.mb-15(style='margin-left:10px !important;margin-right: 10px !important;')
                    .col-6
                        label อีเมล
                        input.form-control(value=(data !== null ? (data.userlogin !== null ? data.userlogin.email : '') : ''), type='email', name='email', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                        span.error-block(data-from='email') กรุณากรอกข้อมูล
                    .col-6
                        label เบอร์โทรศัพท์
                        input.form-control(value=(data !== null ? data.phoneTel:''), maxlength='10', type='text', name='phoneTel', onKeyup="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                        span.error-block(data-from='phoneTel') กรุณากรอกข้อมูล
                .form-row.mb-15(style='margin-left:10px !important;margin-right: 10px !important;')



                    - if(nowSession === 'admin')
                        .col-6
