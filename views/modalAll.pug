                                font(color='red') &nbsp;*
                            select.form-control(name='userlevel', style='font-size:14pt !important;', onChange="this.value.length > 0 ? ($(\"[data-from='\" + this.name +\"']\").removeClass(\"error\") && $(this).removeClass('is-invalid')) : '';")
                                option(value='') เลือกระดับผู้ใช้
                                each value, index in userLv
                                    option(value=value._id, selected=(data !== null ? (data.userlevel._id.toString() === value._id.toString() ? 'true' : false) : false))= value.userlevel
                            span.invalid-feedback.error-block(data-from='userlevel') กรุณากรอกข้อมูล
                    .col-6
                        label(style='margin-right: 7px;') อัพโหลดรูปภาพ
                        br
                        .image-block
                            .blockinput(style='margin-top:10px;')
                                input#inputFileToLoad(type='file', name='fileUpload' )
                                input#pictureString( type='hidden' )
                            .blockpicture
                                img#profile-picture(src=(picture !== null ? 'data:image/png;base64,' + picture : 'data:image/png;base64,') width='200' height='172')
                                canvas#canvas
                //- .form-group.col-12
                //-     input.mr-7(type='checkbox', name='userStatus' checked=(data !== null ? data.userLevelStatus : '')) 
                //-     span สถานะ
            .modal-footer
                button.btn.btn-secondary(type='button', data-dismiss='modal') ปิด
                button.btn.btn-primary(type='button', class=classBtn, data-action=eventText, data-editbyid=editById)= btnText
    script(src='/javascripts/test2.js')






if modalType === 'itemwithdraw'
    .app.thNews
        form#form-order.needs-validation(name='form', novalidate, action='api/itemwithdraw/add', method='post', enctype="multipart/form-data")
            .modal-header(style='background-color:var(--blue);')
                h5#exampleModalLabel.modal-title(style='color:white;')
                    i.far.fa-file-alt.mr-7
                    span= title
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') x
            .modal-body
                .block-content(style='position:relative;')
                    i.far.fa-address-card.mr-7
                    span รายละเอียดของ ลูกค้า/คู่ค้า
                    if data
                        span(style='position:absolute;right:0px;')
                            span.mr-7 สถานะ : 
                            case data.order_status.toLowerCase()
                                when 'pending'
                                    span.badge.badge-warning.mr-7(style='font-size:16pt;') รอการตรวจสอบ
                                    - break
                                when 'approve'
                                    span.badge.badge-success.mr-7(style='font-size:16pt;') อนุมัติ
                                    - break
                                when 'pending'
                                    span.badge.badge-danger.mr-7(style='font-size:16pt;') ไม่อนุมัติ
                                    - break
                            a(href='/backendx/print/'+data._id).thNews.btn.btn-primary.btn-print
                                i.fas.fa-print 
                                    font.thNews พิมพ์
                        //- span(style='float:right;')= data.order_status
                        //- span(style='float:right;')= perm
                        //- span(style='float:right;')= rdOnly
                fieldset(disabled=(data ? data.order_status.toLowerCase() === 'pending' ? !perm ? true : false : true : false))
                    .form-row
                        .col-12
                            label.mb-none ลูกค้า/คู่ค้า
                    .form-row.mb-30
                        .col-12
                            input.form-control(type='text', name='order_by_show', value=(userShowName ? userShowName : ''), data-userid=(data ? data.order_by._id : ''), readonly)
                            input.form-control(type='hidden'
                            , name='order_by'
                            , value=(userId ? userId : '')
                            , data-userid=(data ? data.order_by._id : ''
                            
                            ))

                    .form-row.mb-15
                        .block-content
                            i.fa.fa-shopping-cart.mr-7
                            span รายละเอียดการทำธุรกรรม
                    .form-row.mb-15
                        .col-6
                            label.mb-none เลขที่ใบเบิก
                            input.form-control#order_no(type='text' name='order_no', placeholder='เว้นว่างไว้เพิ่อสร้างโดยอัตโนมัติ', value=(data ? data.order_no : ''), onKeyup="$(\"[data-from='\" + this.name +\"']\").removeClass(\"error\");$(this).removeClass('is-invalid');")
                            span.error-block(data-from='order_no') กรุณาเปลี่ยนเลขที่ใบเบิก เนื่องจากมีข้อมูลอยู่แล้ว
                        .col-6
                            label.mb-none วันที่ทำรายการ
                            input.input-date#order-date.form-control(type='text' name='order_date_show', value=(data ? data.order_date.getDate()+'/'+data.order_date.getMonth()+'/'+(data.order_date.getFullYear()+543) : ''), data-date=(data? data.order_date: false), readonly)
                            input.form-control(type='text' name='order_date', value=(data ? data.order_date : ''), hidden)
                    .form-row.blockAppend.mb-30
                        .col-6
                            label.mb-none รหัสสินค้า
                            #autocomplete.autocomplete
                                input#order_code_show.form-control.autocomplete-input(type='text', name='order_code_show', onclick="$('span[data-from=\"order_code_show\"]').removeClass(\"error\")")
                                ul.autocomplete-result-list
                                span.error-block(data-from='order_code_show') กรุณาเลือกสินค้าที่ต้องการเพิ่ม

                        .col-4
                            label.mb-none จำนวน
                            input.form-control(size="2", type='text' name='order_p_quantity', value='1', onKeyup='if($(this).hasClass("is-invalid")) {$(this).removeClass("is-invalid");$("span.error-block").removeClass("invalid-feedback")};', onkeypress='validNumber(event);')
                            span.error-block(data-from="order_p_quantity")
                        .col-2
                            label.mb-none &nbsp;
                            button#add-list.btn.btn-block.btn-success เพิ่มรายการเบิก
                    .form-row
                        if (dataItem)
                            each x, i in dataItem
                                input.dataArr(type='hidden', value=(x))
                        .col-12.item
                            table.table-responsive-lg#calculateTable(style='width:100%;border-spacing:0; border-collapse:collapse;background-color:')
                                thead.t-center
                                    tr(style='background-color:#ebebeb;')
                                        th รายละเอียด
                                        th จำนวน
                                        th หน่วยละ
                                        th 
                                        th(colspan='2') จำนวนเงิน (บาท)
                                        th
                                tbody#table_product
                                tfoot
                                    tr
                                        td(colspan='3', rowspan="3") 
                                            label.mb-none หมายเหตุ
                                            textarea.form-control(rows="3", name='comment', style='min-width:420px;resize:none;')=(data ? data.comment : '')
                                        //- td.t-right รวม
                                        //- td 
                                        //- td(width='120')
                                        //- td.t-right(style='width:25px;') บาท
                                    tr
                                        td.t-right รวมทั้งสิ้น
                                        td(colspan='2').t-right 
                                            input#order-totalmoney.form-control(value=(data ? orderTotalMoney : '')
                                            , type='text'
                                            , name='order_totalmoney'
                                            , style='min-width:3.2rem !important;border:0px;background-color:#fdf59a;text-align:right;color:brown;cursor:context-menu;'
                                            , readonly)
                                        td.t-right(style='max-width:40px;') บาท
            .modal-footer
                - if(data) 
                    - if(role.toLowerCase() == 'admin')
                        - if(data.order_status.toLowerCase() == 'pending')
                            .mr-auto
                                button.btn.btn-success.btn-approve.mr-7(type='button', data-orderno=(data ? data.order_no : false), data-orderid=(data ? data._id : false), data-userid=(userId ? userId : false)) อนุมัติ
                                button.btn.btn-danger.btn-disapprove(type='button', data-orderno=(data ? data.order_no : false), data-orderid=(data ? data._id : false), data-userid=(userId ? userId : false)) ไม่อนุมัติ
                        - else 
                            .mr-auto 
                                span เปลี่ยนแปลงสถานะโดย : 
                                span=data.order_confirm_by.firstname + ' ' + data.order_confirm_by.lastname
                    - else 
                        - if(data.order_status.toLowerCase() !== 'pending')
                            .mr-auto 
                                span เปลี่ยนแปลงสถานะโดย : 
                                span=data.order_confirm_by.firstname + ' ' + data.order_confirm_by.lastname
                                
                button.btn.btn-secondary(type='button', data-dismiss='modal') ปิด
                - if(data)
                    - if(perm)
                        -if(data.order_status.toLowerCase() === 'pending')
                            button.btn.btn-primary(type='button', class=classBtn, data-action=eventText, data-editbyid=(data ? data._id : false))= btnText
                - else 
                    button.btn.btn-primary(type='button', class=classBtn, data-action=eventText, data-editbyid=(data ? data._id : false))= btnText
            script(src='/javascripts/itemwithdraw.js')